# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key

# Redis Configuration (for BullMQ)
REDIS_URL=redis://localhost:6379

# Anthropic AI Configuration
ANTHROPIC_API_KEY=your-anthropic-api-key

# Google APIs Configuration
GOOGLE_APPLICATION_CREDENTIALS=./service-account.json

# Worker Service Configuration
PORT=3001
NODE_ENV=development
WORKER_API_KEY=your-secure-api-key

# PageSpeed Insights API
PAGESPEED_API_KEY=your-pagespeed-api-key

# SEO Tools APIs
MOZ_ACCESS_ID=your-moz-access-id
MOZ_SECRET_KEY=your-moz-secret-key
SPYFU_API_KEY=your-spyfu-api-key
SEMRUSH_API_KEY=your-semrush-api-key

# Notification Configuration
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/webhook/url
NOTIFICATION_EMAIL=alerts@yourcompany.com

# Rate Limiting
GOOGLE_API_REQUESTS_PER_MINUTE=60
WORDPRESS_API_REQUESTS_PER_MINUTE=30